    },
    "lint-staged": {
        "*.js": [
            "prettier --write ."
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/BB-Code/bag-size.git"
    },
    "keywords": [],
    "author": "BB-Code <15919854639@163.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/BB-Code/bag-size/issues"
    },
    "homepage": "https://github.com/BB-Code/bag-size#readme",
    "devDependencies": {
        "commitizen": "4.2.5",
        "conventional-changelog-cli": "2.2.2",
        "cz-conventional-changelog": "3.3.0",
        "get-folder-size": "2.0.1",
        "husky": "8.0.1",
        "lint-staged": "13.0.3",
        "prettier": "2.7.1",
        "rimraf": "3.0.2",
        "semantic-release": "19.0.3"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
